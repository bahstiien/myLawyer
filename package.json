{
  "name": "mylawyerfront",
  
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "lint": "next lint",
    "generate-db-client": "npx prisma generate",
    "start:server": "next start",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:deploy": "npx prisma migrate deploy",
    "pre-start": "NODE_ENV=production npm-run-all migrate:deploy generate-db-client build ",

    "start": "NODE_ENV=production npm run start:server"
  },
  "dependencies": {
    "@prisma/client": "^3.9.0",
    "antd": "^4.18.5",
    "argon2": "^0.28.3",
    "axios": "^0.25.0",
    "dayjs": "^1.10.7",
    "emailjs-com": "^3.2.0",
    "esm": "^3.2.25",
    "joi": "^17.6.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "nc": "^1.0.3",
    "next": "12.0.10",
    "next-auth": "^4.1.2",
    "next-connect": "^0.11.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.9.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.1.1",
    "safe-json-stringify": "^1.2.0",
    "superjson": "^1.8.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.18"
  },
  "prisma": {
    "seed": "node -r esm -r module-alias/register prisma/seed.js"
  },
  "_moduleAliases": {
    "@styles": "styles",
    "@components": "components",
    "@models": "models",
    "@middlewares": "middlewares",
    "@db": "db.js",
    "@mailer": "mailer.js"
  }
